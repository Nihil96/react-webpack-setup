{
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "semi": false,
  "printWidth": 120,
  "importOrder": [
    "^(assets|components|constants|enums|models|routes|services|styles|utils)/?(.*)$",
    "^[^\\w](?!.*scss).*",
    "\\.(scss)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderParserPlugins": ["jsx", "typescript"]
}
